{
  "name": "ng-cron-editor-app",
  "description": "A cron expression generator to be used in Angular applications. This is a fork of https://github.com/claudiuconstantin/cron-editor to support Angular 17",
  "author": {
    "name": "Shawin Mendis",
    "email": "shawinshawz@gmail.com",
    "url": "https://github.com/ShawinMendis/cron-editor"
  },
  "version": "4.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShawinMendis/cron-editor"
  },
  "bugs": {
    "url": "https://github.com/ShawinMendis/cron-editor/issues"
  },
  "homepage": "https://github.com/ShawinMendis/cron-editor",
  "keywords": [
    "angular",
    "cron",
    "cron-expression",
    "angular-cli",
    "quartz"
  ],
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib": "ng build cron-editor",
    "build": "ng build cron-editor && ng build --prod",
    "start": "yarn run build_lib && ng serve",
    "copy-license": "copy .\\LICENSE.md .\\dist\\cron-editor",
    "copy-readme": "copy .\\README.md .\\dist\\cron-editor",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "pack_lib": "cd dist/cron-editor && npm pack",
    "package": "npm run build_lib && npm run copy-files && npm run pack_lib"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~17.0.0",
    "@angular/common": "~17.0.0",
    "@angular/compiler": "~17.0.0",
    "@angular/core": "~17.0.0",
    "@angular/forms": "~17.0.0",
    "@angular/platform-browser": "~17.0.0",
    "@angular/platform-browser-dynamic": "~17.0.0",
    "@angular/router": "~17.0.0",
    "core-js": "^3.37.1",
    "rxjs": "~7.8.1",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~17.0.0",
    "@angular-devkit/core": "^17.0.0",
    "@angular/cli": "~17.0.0",
    "@angular/compiler-cli": "~17.0.0",
    "@angular/language-service": "~17.0.0",
    "@types/jasmine": "~3.3.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.12.11",
    "bootstrap": "~5.2.0",
    "codelyzer": "~6.0.2",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^17.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.10.2",
    "tslib": "^2.3.0",
    "tslint": "~5.11.0",
    "typescript": "~5.2.2"
  }
}
